@model Teste.EL.Site.Web.Models.AluguelModel

@{
    ViewData["Title"] = "ResumoReserva";
}
<div id="resumo-reserva">
    <div class="row">
        <div class="col-md-12 section-header" style="text-align: left; border-bottom: 1px solid; border-bottom-color: #eaeaea; padding-bottom:10px;">
            <h3 class="title">
                CONFIRMAÇÃO DA RESERVA
            </h3>
        </div>
    </div>
    <div style="text-align:left; text-decoration-color:gray;">
        <font size="3"><i>Listagem</i> > <i>Simulação</i> > <b>Reserva</b> </font>
    </div>
    <br />
    <div>
        <div class="row">
            <div class="col-md-6">
                <div class="imagens">
                    <div class="bx-viewport" aria-live="polite" style="width: 100%; overflow:hidden; position: relative; margin-top:15%; max-height: 400px; max-width:500px;">
                        <center>
                            <img alt="@Model.Placa" title="@Model.Placa" src="data:image/jpeg;base64," />
                        </center>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <form asp-action="ConcluirReserva">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    @Html.HiddenFor(m => m.IdCliente)
                    @Html.HiddenFor(m => m.IdVeiculo)
                    @Html.HiddenFor(m => m.Placa)
                    @Html.HiddenFor(m => m.Categoria)
                    @Html.HiddenFor(m => m.ValorHora)
                    @Html.HiddenFor(m => m.TotalDeHoras)
                    @Html.HiddenFor(m => m.ValorFinalAluguel)
                    @Html.HiddenFor(m => m.DataPrevistaAluguel)
                    <div class="form-group">
                        <label asp-for="Placa" class="control-label"></label>
                        <input asp-for="Placa" class="form-control" disabled="disabled" style="text-align:center;" />
                    </div>
                    <div class="form-group">
                        <label asp-for="Categoria" class="control-label"></label>
                        <input asp-for="Categoria" class="form-control" disabled="disabled" style="text-align:center;" />
                    </div>
                    <div class="form-group">
                        <label asp-for="ValorHora" class="control-label">Valor/Hora</label>
                        <input class="form-control" disabled="disabled" style="text-align:center;" value="@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", Model.ValorHora)" />
                    </div>
                    <div class="form-group">
                        <label asp-for="TotalDeHoras" class="control-label">Total de Horas do Aluguel</label>
                        <input asp-for="TotalDeHoras" class="form-control" disabled="disabled" style="text-align:center;" />
                    </div>
                    <div class="form-group">
                        <label asp-for="ValorFinalAluguel" class="control-label">Valor Final do Aluguel</label>
                        <input class="form-control" disabled="disabled" style="text-align:center;" value="@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", Model.ValorFinalAluguel)" />
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Confirmar!" class="btn btn-success" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<div>
    <a asp-action="Index" asp-controller="Home">Retornar Para Listagem? Clique aqui.</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
